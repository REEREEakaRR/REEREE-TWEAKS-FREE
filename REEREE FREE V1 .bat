@echo off
:: =============================================================================
::  REE REE TWEAKS FREE  |  FULL BUILD (10 sections × 500 tweaks = 5,000 tweaks)
:: =============================================================================
::  NOTE: Run this script as Administrator.
::  A System Restore point is offered before applying tweaks.
:: =============================================================================

:: ---- Admin check ------------------------------------------------------------
fltmc >nul 2>&1 || (
    echo.
    echo   ERROR: Please RIGHT‑CLICK this script and choose "Run as administrator".
    echo.
    pause
    exit /b
)

:: ---- Optional restore point -------------------------------------------------
setlocal EnableDelayedExpansion
choice /m "Create a System Restore point before tweaking?"
if !errorlevel! == 1 (
    powershell -Command "Checkpoint-Computer -Description 'REE REE TWEAKS FREE - Pre‑Tweaks' -RestorePointType 'MODIFY_SETTINGS'"
    echo.
    echo   Restore point created.
    timeout /t 2 >nul
) else (
    echo.
    echo   Skipping restore point.
    timeout /t 1 >nul
)

title REE REE TWEAKS FREE
color 09
mode con: cols=120 lines=40

:mainmenu
cls
echo.
echo  ==========================================================================================================
echo  =====================================  REE  REE  T W E A K S  F R E E  ==================================
echo  ==========================================================================================================
echo.
echo     [1] PERFORMANCE        [2] PRIVACY          [3] SERVICES         [4] NETWORK          [5] GAMING
echo     [6] UI                 [7] EXPLORER         [8] TASKBAR          [9] DEFENDER         [10] STARTUP
echo.
echo     [0] EXIT
echo.
set /p choice=Choose a section [0-10]: 

if "%%choice%%"=="1"  goto performance
if "%%choice%%"=="2"  goto privacy
if "%%choice%%"=="3"  goto services
if "%%choice%%"=="4"  goto network
if "%%choice%%"=="5"  goto gaming
if "%%choice%%"=="6"  goto ui
if "%%choice%%"=="7"  goto explorer
if "%%choice%%"=="8"  goto taskbar
if "%%choice%%"=="9"  goto defender
if "%%choice%%"=="10" goto startup
if "%%choice%%"=="0"  exit
goto mainmenu
